: main
    plane 3
    i := sprite_data_0
    v0 := 4
    v1 := 4
    sprite v0 v1 2
    v1 += 2
    plane 7
    sprite v0 v1 2
    v1 += 2
    plane 15
    sprite v0 v1 2

    plane 1
    v0 := 16
    v1 := 0
    :call draw-circles
    plane 2
    :call draw-circles

: key-loop
    v2 := key
    if v2 == 5 then
      scroll-up 1
    if v2 == 7 then
      scroll-left
    if v2 == 8 then
      scroll-down 1
    if v2 == 9 then
      scroll-right
    jump key-loop

: draw-circles
    i := circles-0-0
    sprite v0 v1 0
    v0 += 16
    i := circles-1-0
    sprite v0 v1 0
    v0 -= 16
    v1 += 16
    i := circles-0-1
    sprite v0 v1 0
    v0 += 16
    i := circles-1-1
    sprite v0 v1 0
    v0 -= 16
    v1 -= 16
    return

